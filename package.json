{
  "name": "lp-compat",
  "author": "Flixbox",
  "private": true,
  "type": "module",
  "description": "Monorepo to show Lucky Patcher community documentation",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/Flixbox/lp-compat.git"
  },
  "bugs": {
    "url": "https://github.com/Flixbox/lp-compat/issues"
  },
  "homepage": "https://github.com/Flixbox/lp-compat#readme",
  "version": "0.0.1",
  "engines": {
    "bun": ">=1.3.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "bun run --filter './packages/*' start",
    "format": "biome format --write .",
    "lint": "bunx npm-run-all --continue-on-error \"lint:*\"",
    "lint:common": "biome lint .",
    "lint:typecheck": "bun run --filter './packages/*' lint:typecheck",
    "lint:pkgjson": "bunx npm-package-json-lint .",
    "lint:knip": "knip",
    "check": "biome check --write .",
    "build": "bun run --filter './packages/*' build",
    "postinstall": "touch tsconfig.json"
  },
  "devDependencies": {
    "@arthurrios/biome-config": "1.1.4",
    "@biomejs/biome": "2.3.4",
    "@types/bun": "latest",
    "@types/node": "24.10.0",
    "knip": "5.68.0",
    "npm-package-json-lint": "9.0.0"
  },
  "dependencies": {
    "typescript": "5.9.3"
  }
}